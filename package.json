{
  "name": "userstyles",
  "version": "2.6.1",
  "homepage": "https://n2o.io/go?page=userstyles",
  "license": "MIT",
  "author": "Francis Zhao <francis@n2o.io>",
  "repository": "github:francis-zhao/userstyles",
  "type": "module",
  "scripts": {
    "build": "yarn run build:sass && yarn run build:postcss && yarn run format",
    "build:sass": "sass --no-charset --no-source-map src:dist",
    "build:postcss": "postcss dist/**/*.css --replace",
    "format": "prettier --write --cache --cache-location .yarn/cache/prettier/.prettier-cache --cache-strategy metadata .",
    "yarn-sdks:vscode": "yarn dlx @yarnpkg/sdks vscode"
  },
  "devDependencies": {
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-preset-env": "^9.5.2",
    "prettier": "^3.2.5",
    "sass": "^1.72.0"
  },
  "browserslist": [
    "last 3 years",
    "not dead"
  ]
}
